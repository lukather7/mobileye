<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside> 
  <div class="col-md-8">
    <table class="table table-striped table-bordered">
      <tr>
        <thead>
          <td>Car-ID</td> <td>Company</td> <td>Number</td>
          <td>Edit</td> <td>Carte</td>
        </thead>
      </tr>
      <tbody>
        <% @carids.each do |car| %>
          <tr>
           <td><%= car.id %></td> 
           <td><%= car.company %></td> 
           <td><%= car.number %></td> 
           <td><%= link_to :Edit, edit_carid_path(car.id), class:"btn btn-primary btn-xs" %></td>
           <td><%= link_to :Carte, carte_carid_path(car.id), class:"btn btn-success btn-xs" %></td>
          </tr>
        
        <% end %> 
      </tbody>
    </table>
  </div>
</div>

